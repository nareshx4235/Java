package oops;
interface Animal{
	public void walk();
	
        }


 interface Herbivore {        
	 
 }

class Horse implements Animal,Herbivore  {  //multiple inheritance 
	
	
	public void walk() {
	
	System.out.println("walks on 4 legs");
	
         }
      }


public class OOPS {
	public static void main(String args[]) {
		Horse horse = new Horse();
		horse.walk();
	}

}
